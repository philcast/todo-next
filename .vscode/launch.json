{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run dev",
      "command": "npm run dev",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "Run application",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"],
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
